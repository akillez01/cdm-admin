{
  "name": "cdm-admin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://akillez01.github.io/cdm-admin",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --config vite.config.production.ts",
    "build:plesk": "npm run build:production",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:production": "vite preview --config vite.config.production.ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:plesk": "echo 'Build completed! Upload dist/ folder to your Plesk server.'",
    "migrate:export": "node scripts/migrateToMySQL.mjs",
    "migrate:help": "echo 'Use: npm run migrate:export - Exporta dados do Supabase para MySQL'",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "server:install": "cd server && npm install"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.6",
    "@supabase/supabase-js": "^2.39.7",
    "dotenv": "^16.6.0",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-datepicker": "^8.3.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.3.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
